    <canopy>
      <name>canopy</name>
      <mode>in-transit</mode>
      <path>Aircraft/A-10/Sounds/canopy-trans.wav</path>
      <property>canopy/position-norm</property>
      <volume>
        <factor>0.04</factor>
      </volume>
    </canopy>

    <canopy-close>
      <name>canopy-close2</name>
      <mode>once</mode>
      <path>Aircraft/A-10/Sounds/canopy-close2.wav</path>
      <condition>
        <less-than-equals>
          <property>canopy/position-norm</property>
          <value>0</value>
        </less-than-equals>
      </condition>
      <volume>
        <factor>0.09</factor>
      </volume>
    </canopy-close>


    <!-- stall warnings -->
    <pre-stall>
      <name>pre-stall</name>
      <mode>looped</mode>
      <path>Aircraft/A-10/Sounds/stall-600-chopped.wav</path>
      <condition>
        <greater-than>
          <property>systems/electrical/outputs/aoa-indexer</property>
          <value>23</value>
        </greater-than>
        <greater-than>
          <property>orientation/alpha-deg</property>
          <value>10</value>
        </greater-than>
        <less-than-equals>
          <property>orientation/alpha-deg</property>
          <value>13.5</value>
        </less-than-equals>
        <greater-than>
          <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
          <value>10</value>
        </greater-than>
      </condition>
      <volume>
        <value>1</value>
      </volume>
    </pre-stall>


    <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>Aircraft/A-10/Sounds/stall-600-continue.wav</path>
      <condition>
        <greater-than>
          <property>systems/electrical/outputs/aoa-indexer</property>
          <value>23</value>
        </greater-than>
        <greater-than>
          <property>orientation/alpha-deg</property>
          <value>13.5</value>
        </greater-than>
        <greater-than>
          <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
          <value>10</value>
        </greater-than>
      </condition>
      <volume>
        <value>1</value>
      </volume>
    </stall>

    <!-- gun -->
    <gun> 
      <name>gun</name>
      <mode>looped</mode>
      <path>Aircraft/A-10/Sounds/gun2.wav</path>
      <condition> 
        <property>ai/submodels/GAU-8A[0]</property>
      </condition>
      <volume> 
        <value>1</value>
      </volume>
    </gun>

    <!-- nav selector -->
    <nav-selector-ptr-able> 
      <name>nav-selector-ptr-able</name>
      <path>Aircraft/A-10/Sounds/click.wav</path>
      <condition> 
        <property>A-10-nav/A-10-nav-selector-ptr</property>
      </condition>
      <volume> 
        <factor>0.4</factor>
      </volume>
    </nav-selector-ptr-able>
    <nav-selector-ptr-stow> 
      <name>nav-selector-ptr-stow</name>
      <path>Aircraft/A-10/Sounds/click.wav</path>
      <condition> 
        <not> 
          <property>A-10-nav/A-10-nav-selector-ptr</property>
        </not>
      </condition>
      <volume> 
        <factor>0.4</factor>
      </volume>
    </nav-selector-ptr-stow>

    <nav-selector-tacan-on> 
      <name>nav-selector-tacan-on</name>
      <path>Aircraft/A-10/Sounds/click2.wav</path>
      <condition> 
        <property>instrumentation/tacan/switch-position</property>
      </condition>
      <volume> 
        <factor>0.4</factor>
      </volume>
    </nav-selector-tacan-on>
    <nav-selector-tacan-off> 
      <name>nav-selector-tacan-off</name>
      <path>Aircraft/A-10/Sounds/click2.wav</path>
      <condition> 
        <not> 
          <property>instrumentation/tacan/switch-position</property>
        </not>
      </condition>
      <volume> 
        <factor>0.4</factor>
      </volume>
    </nav-selector-tacan-off>

    <!-- throttle panel -->
    <apu-switch-start> 
      <name>apu-switch-start</name>
      <mode>once</mode>
      <path>Aircraft/A-10/Sounds/click.wav</path>
      <condition> 
        <property>controls/APU/off-start-switch</property>
      </condition>
      <volume> 
        <factor>0.3</factor>
      </volume>
    </apu-switch-start>
    <apu-switch-stop> 
      <name>apu-switch-stop</name>
      <mode>once</mode>
      <path>Aircraft/A-10/Sounds/click.wav</path>
      <condition> 
        <not> 
          <property>controls/APU/off-start-switch</property>
        </not>
      </condition>
      <volume> 
        <factor>0.3</factor>
      </volume>
    </apu-switch-stop>


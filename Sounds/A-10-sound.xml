<?xml version="1.0"?>

<PropertyList>

  <fx>

    <!-- engines -->
    <engine>
      <name>engine</name>
      <path>Sounds/jet.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine[0]/running</property>
      </condition>
      <volume>
        <property>/engines/engine[0]/prop-thrust</property>
        <factor type="double">0.0001</factor>
      </volume>
      <pitch>
        <property>/engines/engine[0]/n1</property>
        <factor>0.2</factor>
      </pitch>
    </engine>

    <engine>
      <name>whine</name>
      <path>Sounds/whine.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine[0]/running</property>
      </condition>
      <volume>
        <property>/engines/engine[0]/n2</property>
        <factor>0.0005</factor>
      </volume>
      <pitch>
        <property>/engines/engine[0]/n2</property>
        <factor>0.01</factor>
        <offset>0.15</offset>
      </pitch>
    </engine>

    <engine>

   <action>
    <name>freq-x10</name>
    <button>0</button>
    <x>-165</x>
    <y>335</y>
    <w>90</w>
    <h>65</h>
    <binding>
     <command>property-adjust</command>
     <property>sim/model/A-10/instrumentation/nav[2]/frequencies/alt-selected-mhz-x10</property>
     <step>1</step>
     <min>1</min>
     <max>16</max>
     <resolution>1</resolution>
     <wrap>true</wrap>
    </binding>
    <binding>
     <command>nasal</command>
     <script>nav_scripts.alt_freq_to_freq();</script>
    </binding>
   </action>

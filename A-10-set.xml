	<input>

		<keyboard>
			<key n="9">
				<name>Ctrl-I</name>
				<desc>Show configuration dialog</desc>
				<binding>
					<command>nasal</command>
					<script>A10weapons.config_dialog.toggle()</script>
				</binding>
			</key>
			<key n="44">
				<name>,</name>
				<desc>Left brake</desc>
				<binding>
					<command>nasal</command>
					<script>A10weapons.release()</script>
				</binding>
			</key>
			<key n="67">
				<name>C</name>
				<desc>Toggle canopy on or off</desc>
				<binding>
				   <command>nasal</command>
				   <script>canopy.canopy_switch(2)</script>
				</binding>
			</key>
			<key n="101">
				<name>e</name>
				<desc>Fire GAU-8/A cannon</desc>
				<binding>
					<command>nasal</command>
					<script>A10weapons.fire_gau8a()</script>
				</binding>
				<mod-up>
					<binding>
						<command>nasal</command>
						<script>A10weapons.cfire_gau8a()</script>
					</binding>
				</mod-up>
			</key>
			<key n="102">
				<name>f</name>
				<desc>Extend Slats.</desc>
				<binding>
					<command>property-adjust</command>
					<property>controls/flight/slats</property>
				<step type="double">0.25</step>
				</binding>
			</key>
			<key n="75">
				<name>K</name>
				<desc>Toggle trajectory markers</desc>
				<binding n="0">
					<command>nasal</command>
					<script>A10.toggle_traj_mkr()</script>
				</binding>
			</key>
			<!-- Update Drop View position -->
			<key n="85">
				<name>U</name>
				<desc>Update Drop View position</desc>
				<binding n="0">
					<command>nasal</command>
					<script>A10.update_drop_view_pos()</script>
				</binding>
			</key>
		</keyboard>


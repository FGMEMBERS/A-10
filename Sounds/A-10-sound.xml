<?xml version="1.0"?>

<PropertyList>

  <fx>

    <!-- engines outside-->
    <engine>
      <name>engine-outside</name>
      <path>Sounds/jet.wav</path>
      <mode>looped</mode>
      <condition>
          <or>
            <not>
              <equals>
                <property>sim/current-view/view-number</property>
                <value>0</value>
              </equals>
            </not>
            <greater-than>
              <property>canopy/position-norm</property>
              <value>0</value>
            </greater-than>
          </or>
      </condition>
      <volume>
        <property>engines/engine[0]/A-10-alt-prop-thrust</property>
        <factor type="double">0.0001</factor>
      </volume>
      <pitch>
        <property>engines/engine[0]/A-10-alt-n1</property>
        <factor>0.2</factor>
      </pitch>
    </engine>

    <engine>
      <name>whine-outside</name>
      <path>Sounds/whine.wav</path>
      <mode>looped</mode>
      <condition>
          <or>
            <not>
              <equals>
                <property>sim/current-view/view-number</property>
                <value>0</value>
              </equals>
            </not>
            <greater-than>
              <property>canopy/position-norm</property>
              <value>0</value>
            </greater-than>
          </or>
      </condition>
      <volume>
        <property>engines/engine[0]/A-10-alt-n2</property>
        <factor>0.0005</factor>
      </volume>
      <pitch>
        <property>engines/engine[0]/A-10-alt-n2</property>
        <factor>0.01</factor>
        <offset>0.15</offset>
      </pitch>
    </engine>

    <engine>
      <name>engine-outside</name>
      <path>Sounds/jet.wav</path>
      <mode>looped</mode>
      <condition>
          <or>
            <not>
              <equals>
                <property>sim/current-view/view-number</property>
                <value>0</value>
              </equals>
            </not>
            <greater-than>
              <property>canopy/position-norm</property>
              <value>0</value>
            </greater-than>
          </or>
      </condition>
      <volume>
        <property>engines/engine[1]/A-10-alt-prop-thrust</property>
        <factor type="double">0.0001</factor>
      </volume>
      <pitch>
        <property>engines/engine[1]/A-10-alt-n1</property>
        <factor>0.2</factor>
      </pitch>
    </engine>

    <engine>
      <name>whine-outside</name>
      <path>Sounds/whine.wav</path>
      <mode>looped</mode>
      <condition>
          <or>
            <not>
              <equals>
                <property>sim/current-view/view-number</property>
                <value>0</value>
              </equals>
            </not>
            <greater-than>
              <property>canopy/position-norm</property>
              <value>0</value>
            </greater-than>
          </or>
      </condition>
      <volume>
        <property>engines/engine[1]/A-10-alt-n2</property>
        <factor>0.0005</factor>
      </volume>
      <pitch>
        <property>engines/engine[1]/A-10-alt-n2</property>
        <factor>0.01</factor>
        <offset>0.15</offset>
      </pitch>
    </engine>

    <!-- engines inside -->
    <engine>
      <name>engine-inside</name>
      <path>Sounds/jet.wav</path>
      <mode>looped</mode>
      <condition>
          <equals>
            <property>sim/current-view/view-number</property>
            <value>0</value>
            </equals>
          <less-than-equals>
            <property>canopy/position-norm</property>

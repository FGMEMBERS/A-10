<?xml version="1.0"?>

  <PropertyList> 
    <path>A-10-engines-gauges.ac</path>


<!-- temp -->
    <animation>
      <name>temp-l</name>
      <type>rotate</type>
      <object-name>needle-temp-l</object-name>
      <property>engines/engine[0]/itt-norm</property>
      <interpolation> 
        <entry><ind>0.0000</ind><dep>118</dep></entry><!--0-->
        <entry><ind>0.0213</ind><dep>179</dep></entry><!--iddle-->
        <entry><ind>0.2310</ind><dep>164</dep></entry><!--25% thrust-->
        <entry><ind>0.9870</ind><dep>295</dep></entry><!--max thrust-->
      </interpolation>
      <center>
       <x-m>0.0047</x-m>
       <y-m>-0.0781</y-m>
       <z-m>0.0547</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation>
      <name>temp-r</name>
      <type>rotate</type>
      <object-name>needle-temp-r</object-name>
      <property>engines/engine[1]/itt-norm</property>
      <interpolation> 
        <entry><ind>0.0000</ind><dep>118</dep></entry><!--0-->
        <entry><ind>0.0213</ind><dep>179</dep></entry><!--iddle-->
        <entry><ind>0.2310</ind><dep>164</dep></entry><!--25% thrust-->
        <entry><ind>0.9870</ind><dep>295</dep></entry><!--max thrust-->
      </interpolation>
      <center>
       <x-m>0.0047</x-m>
       <y-m>-0.0263</y-m>
       <z-m>0.0547</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>


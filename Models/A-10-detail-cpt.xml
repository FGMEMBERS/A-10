<?xml version="1.0"?>


<PropertyList>

 <path>detail-cpt.ac</path>

<!-- panels-light-weak -->


  <animation> 
   <object-name>CPT-video-frame-l</object-name>
   <object-name>CPT-video-frame-r</object-name>
   <object-name>CPT-video-screen</object-name>
   <type>material</type>
    <emission> 
    <factor-prop>controls/lighting/panel-norm</factor-prop>
     <red>0.45</red>
     <green>0.34</green>
     <blue>0.18</blue>
    </emission>
  </animation>


	<!-- aux-lg-ext-handle -->
	<animation>
		<type>translate</type>
		<object-name>aux-lg-ext-handle</object-name>
		<property>sim/model/A-10/controls/gear/aux-lg-ext</property>
		<factor>0.04</factor>
		<axis>
			<x>0.1372</x>
			<y>0.00</y>
			<z>0.0232</z>
		</axis>
	</animation>

	<animation>
		<object-name>detail-cpt</object-name>
		<type>noshadow</type>
	</animation>

</PropertyList>

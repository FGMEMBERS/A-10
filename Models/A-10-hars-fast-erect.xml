<?xml version="1.0"?>

<PropertyList>

 <path>A-10-hars-fast-erect.ac</path>

<!-- lighting -->

  <animation> 
   <object-name>mount</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation> 
   <object-name>button</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.10</red>
        <green>0.05</green>
        <blue>0.025</blue>
      </emission>
  </animation>

  <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>

</PropertyList>

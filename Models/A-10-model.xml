<!-- De-select pilot when in view 0 -->
 <animation>
  <type>select</type>
  <object-name>CPT-Pilot-Head</object-name>
  <object-name>CPT-Pilot-Body</object-name>
  <object-name>CPT-Helmet</object-name>
  <object-name>CPT-Visor</object-name>
  <object-name>CPT-Face-Mask</object-name>
  <object-name>CPT-C-Stick</object-name><!--otherwise it's just hidding HSI-->
  <condition>
   <not>
   <equals>
    <property>sim/current-view/view-number</property>
    <value>0</value>
   </equals>
   </not>
  </condition>
 </animation>

<!-- Canopy -->
 <animation>
  <type>rotate</type>
  <object-name>BCanopyFrame</object-name>
  <object-name>BCanopyFrameInner</object-name>
  <object-name>BCanopyGlass</object-name>
  <property>canopy/position-norm</property>
  <factor>50</factor>
  <center>
   <x-m>4.65</x-m>
   <y-m>0.0</y-m>
   <z-m>1.4</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<!-- Rudder bits -->
 <animation>
  <type>rotate</type>
  <object-name>LRudder-L</object-name>
  <object-name>LRudder-R</object-name>
  <object-name>LRudder-S</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <factor>-20</factor>
  <center>
   <x-m>15.18</x-m>
   <y-m>-2.87</y-m>
   <z-m>0.14</z-m>
  </center>
  <axis>
   <x>-0.1</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>

<!-- Select the external loads as appropriate -->
<!-- Select extermal tanks -->
 <animation>
  <type>select</type>
  <object-name>FTank3</object-name>
  <condition>
   <equals>
    <property>/consumables/fuel/tank[3]/selected</property>
    <value>1</value>
   </equals>
  </condition>
 </animation>


    <!-- stall warnings -->
    <pre-stall>
      <name>pre-stall</name>
      <mode>looped</mode>
      <path>Aircraft/A-10/Sounds/stall-600-chopped.wav</path>
      <condition>
        <and>
          <and>
            <greater-than>
              <property>/orientation/alpha-deg</property>
              <value>10</value>
            </greater-than>
            <less-than-equals>
              <property>/orientation/alpha-deg</property>
              <value>13.5</value>
            </less-than-equals>
          </and>
          <greater-than>
            <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
            <value>10</value>
          </greater-than>
        </and>
      </condition>
      <volume>
        <value>1</value>
      </volume>
    </pre-stall>

    <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>Aircraft/A-10/Sounds/stall-600-continue.wav</path>
      <condition>
        <and>
          <greater-than>
            <property>/orientation/alpha-deg</property>
            <value>13.5</value>
          </greater-than>
          <greater-than>
            <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
            <value>10</value>
          </greater-than>
        </and>
      </condition>
      <volume>
        <value>1</value>
      </volume>
    </stall>


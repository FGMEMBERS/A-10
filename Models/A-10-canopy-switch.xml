<?xml version="1.0"?>


<PropertyList>

  <path>A-10-canopy-switch.ac</path>

  <animation>
    <object-name>switch</object-name>
    <type>rotate</type>
 	  <property>controls/canopy/A-10-canopy-switch</property>
    <factor>30</factor>
    <center>
      <x-m>2.4829</x-m>
      <y-m>0.4717</y-m>
      <z-m>0.8888</z-m>
	  </center>
	  <axis>
      <x>-1</x>
      <y>-1</y>
      <z>0</z>
	  </axis>
  </animation>


<!-- lighting -->

  <animation> 
    <object-name>mount</object-name>
    <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
      <red>0.45</red>
      <green>0.34</green>
      <blue>0.18</blue>
      </emission>
  </animation>

  <animation> 
    <object-name>switch</object-name>
    <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
      <red>0.20</red>
      <green>0.10</green>
      <blue>0.05</blue>
    </emission>
  </animation>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>5</max-m>
  </animation>

</PropertyList>

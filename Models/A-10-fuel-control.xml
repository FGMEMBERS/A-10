<?xml version="1.0"?>


<PropertyList>

	<path>A-10-fuel-control.ac</path>


<!-- AAR receiver lever -->
	<animation>
		<object-name>rcvr-handle</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/fuel/receiver-lever</property>
		<interpolation>
			<entry><ind>0</ind><dep>30</dep></entry>
			<entry><ind>1</ind><dep>-30</dep></entry>
		</interpolation>
		<center>
			<x-m>0.0283</x-m>
			<y-m>0.0735</y-m>
			<z-m>-0.0505</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation> 
		<object-name>ext-tks-wing-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/fuel/ext-tanks-wing</property>
		<factor>60</factor>
		<center>
			<x-m>-0.0634</x-m>
			<y-m>-0.0520</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

